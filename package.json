{
  "name": "frameX",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --filter=!@framex/multi-tenant-ssl",
    "dev:all": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "turbo": "^2.7.5",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@10.28.2",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "http-status-codes": "^2.3.0"
  }
}